{
  "hosting": {
    "public": "./dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/moreDetailsService",
        "destination": "/moreDetailsService.html"
      },
      {
        "source": "/moreDetailsPage",
        "destination": "/moreDetailsPage.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
